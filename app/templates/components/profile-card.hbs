<div class="row">{{this.boldTabs}}
  <div class="col-sm-10 bg-white border border-radius-1 pt-5 mx-4">
    <ProfilePicture @class="row" @style="profile-page" @url={{this.imgUrl}} @where={{@where}} />
    {{!-- <div class="row justify-content-center mt-4">
      <div class="col-sm-10">
        <h4 class="font-weight-bold text-center primary-text-color">
          IWEARBLACK5
        </h4>
      </div>
    </div>
    <div class="row justify-content-center border-bottom">
      <div class="col-sm-10 mt-2 mb-4">
        <h5 class="text-center">
          <small class="font-weight-light primary-text-color">
            Bskbsd234@gmail.com
          </small>
        </h5>
      </div>
    </div> --}}
    {{#if (eq @where "admin")}}
      <div class="row justify-content-center mt-4">
        <div class="col-sm-10">
          <h4 class="font-weight-bold text-center primary-text-color">
            ADMIN
          </h4>
        </div>
      </div>
      <LinkTo @route="user.profile" @class="row justify-content-center profile-card-linkto">
        <div class="col-sm-10 my-3">
          <h5 id="user-profile-tab" class="primary-text-color">
            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="#384170" class="bi bi-person mr-2" viewBox="0 0 16 16">
              <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0zm4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4zm-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10z"/>
            </svg>
            <span class="profile-card-text">User Profile</span>
          </h5>
        </div>
      </LinkTo>
      <LinkTo @route="admin.transaction-list" @class="row justify-content-center profile-card-linkto">
        <div class="col-sm-10 my-3">
          <h5 id="transaction-tab" class="primary-text-color">
            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25"
              fill="currentColor" class="bi bi-bag-check" viewBox="0 0 16 16">
              <path fill-rule="evenodd"
                d="M10.854 8.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L7.5 10.793l2.646-2.647a.5.5 0 0 1 .708 0z" />
              <path
                d="M8 1a2.5 2.5 0 0 1 2.5 2.5V4h-5v-.5A2.5 2.5 0 0 1 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5zM2 5h12v9a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V5z" />
            </svg>
            Transaction List
          </h5>
        </div>
      </LinkTo>
      <LinkTo @route="admin.seller-list" @class="row justify-content-center profile-card-linkto">
        <div class="col-sm-10 my-3">
          <h5 id="seller-tab" class="primary-text-color">
            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-bag-check" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M10.854 8.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L7.5 10.793l2.646-2.647a.5.5 0 0 1 .708 0z" />
              <path d="M8 1a2.5 2.5 0 0 1 2.5 2.5V4h-5v-.5A2.5 2.5 0 0 1 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5zM2 5h12v9a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V5z" />
            </svg>
            Seller List
          </h5>
        </div>
      </LinkTo>
    {{else}}
      <LinkTo @route="user.profile" @class="row justify-content-center profile-card-linkto">
        <div class="col-sm-10 my-3">
          <h5 id="user-profile-tab" class="primary-text-color">
            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="#384170" class="bi bi-person mr-2" viewBox="0 0 16 16">
              <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0zm4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4zm-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10z"/>
            </svg>
            <span class="profile-card-text">User Profile</span>
          </h5>
        </div>
      </LinkTo>
      <LinkTo @route="user.my-orders" @class="row justify-content-center">
        <div class="col-sm-10 my-3">
          <h5 id="user-orders-tab" class="primary-text-color">
            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-bag-check mr-2" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M10.854 8.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L7.5 10.793l2.646-2.647a.5.5 0 0 1 .708 0z" />
              <path d="M8 1a2.5 2.5 0 0 1 2.5 2.5V4h-5v-.5A2.5 2.5 0 0 1 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5zM2 5h12v9a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V5z" />
            </svg>
            <span class="profile-card-text">My Order</span>
          </h5>
        </div>
      </LinkTo>
      {{#if this.isSeller}}
        <LinkTo @route="seller.profile" @class="row justify-content-center profile-card-linkto">
          <div class="col-sm-10 my-3">
            <h5 id="seller-profile-tab" class="primary-text-color">
              <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="#384170" class="bi bi-person mr-2" viewBox="0 0 16 16">
                <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0zm4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4zm-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10z"/>
              </svg>
              <span class="profile-card-text">Seller Profile</span>
            </h5>
          </div>
        </LinkTo>
        <LinkTo @route="seller.products-list" @class="row justify-content-center profile-card-linkto">
          <div class="col-sm-10 my-3">
            <h5 id="parts-tab" class="primary-text-color">
              <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="#384170" class="bi bi-speedometer mr-2" viewBox="0 0 16 16">
                <path d="M8 2a.5.5 0 0 1 .5.5V4a.5.5 0 0 1-1 0V2.5A.5.5 0 0 1 8 2zM3.732 3.732a.5.5 0 0 1 .707 0l.915.914a.5.5 0 1 1-.708.708l-.914-.915a.5.5 0 0 1 0-.707zM2 8a.5.5 0 0 1 .5-.5h1.586a.5.5 0 0 1 0 1H2.5A.5.5 0 0 1 2 8zm9.5 0a.5.5 0 0 1 .5-.5h1.5a.5.5 0 0 1 0 1H12a.5.5 0 0 1-.5-.5zm.754-4.246a.389.389 0 0 0-.527-.02L7.547 7.31A.91.91 0 1 0 8.85 8.569l3.434-4.297a.389.389 0 0 0-.029-.518z"/>
                <path fill-rule="evenodd" d="M6.664 15.889A8 8 0 1 1 9.336.11a8 8 0 0 1-2.672 15.78zm-4.665-4.283A11.945 11.945 0 0 1 8 10c2.186 0 4.236.585 6.001 1.606a7 7 0 1 0-12.002 0z"/>
              </svg>
              <span class="profile-card-text">Part List</span>
            </h5>
          </div>
        </LinkTo>
      {{/if}}
    {{/if}}
  </div>
</div>